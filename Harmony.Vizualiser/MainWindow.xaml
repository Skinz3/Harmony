            <Menu Name="menu" DockPanel.Dock="Top" Height="28" >
                <MenuItem  Header="_File" Height="28" Width="42">
                    <MenuItem  Header="_New"  Foreground="#FFF1F1F1"   />
                </MenuItem>
            </Menu>
        </DockPanel>
</Window>
